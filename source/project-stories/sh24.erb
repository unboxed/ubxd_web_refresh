---

<header class="page-header">
  <div class="page-header__container">
    <h1 class="page-header__title">
      <%= current_page.data.header.introduction %>
    </h1>

    <p class="page-header__client">
      <%= current_page.data.header.client %>
    </p>
  </div>
</header>

<div class="container">
  <img class="full-image" <%= retina_src current_page.data.header.image %> id="siteHeaderWaypoint" alt="Sexual Health post it" />

  <script>
  new Waypoint({
    element: $('#siteHeaderWaypoint'),
    handler: function(direction) {
      var siteHeader = $('.site-header');
      if (direction === 'down') {
        siteHeader.addClass('site-header--active-scroll');
      } else {
        siteHeader.removeClass('site-header--active-scroll');
      }
    },
    offset: 54
  });
  </script>

  <section class="component">
    <div class="component__tile text-tile">
      <h2 class="text-tile__title">
        <%= current_page.data.introduction.title %>
      </h2>

      <p class="text-tile__paragraph">
        <%= current_page.data.introduction.content %>
      </p>
    </div>

    <div class="component__tile image-tile">
      <img class="image-tile__image" <%= retina_src current_page.data.introduction.image %> alt="<%= current_page.data.introduction.image_description %>" />
    </div>
  </section>

  <section class="bitesize">
    <h2 class="bitesize__title">
      <%= current_page.data.statistics_title %>
    </h2>

    <ul class="bitesize__list">
      <% current_page.data.statistics.each do |statistic| %>
        <li class="bitesize__list-item">
          <%= render_markdown statistic %>
        </li>
      <% end %>
    </ul>
  </section>

  <section class="component">
    <div class="component__tile text-tile">
      <h2 class="text-tile__title">
        <%= current_page.data.launch.title %>
      </h2>

      <p class="text-tile__paragraph">
        <%= current_page.data.launch.content %>
      </p>
    </div>

    <div class="component__tile image-tile">
      <img class="image-tile__image" <%= retina_src current_page.data.launch.image %> alt="<%= current_page.data.launch.image_description %>" />
    </div>
  </section>

  <blockquote class="quote">
    <p class="quote__text">
      <%= current_page.data.first_met_quote.quote %>
    </p>
    <p class="quote__source">
      <%= current_page.data.first_met_quote.source %>
    </p>
  </blockquote>

  <section class="component">
    <div class="component__tile text-tile">
      <h2 class="text-tile__title">
        <%= current_page.data.service.title %>
      </h2>

      <p class="text-tile__paragraph">
        <%= current_page.data.service.content %>
      </p>
    </div>

    <div class="component__tile image-tile">
      <img class="image-tile__image" <%= retina_src current_page.data.service.image %> alt="<%= current_page.data.service.image_description %>" />
    </div>
  </section>

  <a href="https://www.youtube.com/watch?v=<%= current_page.data.blood_instruction_video.youtube_id %>" target="_blank">
    <img class="full-image" <%= retina_src current_page.data.blood_instruction_video.thumbnail %> alt="Blood instructions video" id="bloodInstructionVideoThumbnail" />
  </a>

  <script>
    var $embed = $('<div class="full-video"><iframe id="bloodInstructionVideo" src="https://www.youtube.com/embed/<%= current_page.data.blood_instruction_video.youtube_id %>?rel=0&amp&amp;showinfo=0" frameborder="0" allowfullscreen></iframe></div>');
    $("#bloodInstructionVideoThumbnail").replaceWith($embed);
  </script>

  <section class="component">
    <div class="component__tile text-tile">
      <h2 class="text-tile__title">
        <%= current_page.data.learning.title %>
      </h2>

      <p class="text-tile__paragraph">
        <%= current_page.data.learning.content %>
      </p>
    </div>

    <div class="component__tile image-tile">
      <img class="image-tile__image" <%= retina_src current_page.data.learning.image %> alt="<%= current_page.data.learning.image_description %>" />
    </div>
  </section>

  <blockquote class="quote">
    <p class="quote__text">
      <%= current_page.data.timely_quote.quote %>
    </p>
    <p class="quote__source">
      <%= current_page.data.timely_quote.source %>
    </p>
  </blockquote>

  <img class="full-image" <%= retina_src current_page.data.sh24_banner.image %> alt="<%= current_page.data.sh24_banner.image_description %>" />

  <section class="component">
    <div class="component__tile text-tile">
      <h2 class="text-tile__title">
        <%= current_page.data.stakeholder_trust.title %>
      </h2>

      <p class="text-tile__paragraph">
        <%= current_page.data.stakeholder_trust.content %>
      </p>
    </div>

    <div class="component__tile image-tile">
      <img class="image-tile__image" <%= retina_src current_page.data.stakeholder_trust.image %> alt="<%= current_page.data.stakeholder_trust.image_description %> " />
    </div>
  </section>

  <blockquote class="quote">
    <p class="quote__text">
      <%= current_page.data.team_quote.quote %>
    </p>
    <p class="quote__source">
      <%= current_page.data.team_quote.source %>
    </p>
  </blockquote>

  <img class="full-image" <%= retina_src current_page.data.proposition.image %> alt="<%= current_page.data.proposition.image_description %>" />

  <section class="bitesize">
    <h2 class="bitesize__title">
      <%= current_page.data.conclusion.title %>
    </h2>

    <section class="bitesize__section">
      <h3 class="bitesize__sub-title">
        <%= current_page.data.conclusion.design_and_build_title %>
      </h3>

      <ul>
        <% current_page.data.conclusion.design_and_build.each do |statistic| %>
          <li class="bitesize__list-item">
            <%= render_markdown statistic %>
          </li>
        <% end %>
      </ul>
    </section>

    <section class="bitesize__section">
      <h3 class="bitesize__sub-title">
        <%= current_page.data.conclusion.since_launch_title %>
      </h3>

      <ul>
        <% current_page.data.conclusion.since_launch.each do |statistic| %>
          <li class="bitesize__list-item">
            <%= render_markdown statistic %>
          </li>
        <% end %>
      </ul>
    </section>
  </section>

  <section class="component">
    <a class="link-tile link-tile--site-link" href="<%= current_page.data.site.link %>" target="_blank">
      <div>
        <div class="link-tile__type">
          link
        </div>
        <div class="link-tile__title">
          <%= current_page.data.site.text %>
        </div>
      </div>
    </a>
    <% current_page.data.links.each do |link| %>
      <a class="link-tile" href="<%= link.link %>" target="_blank">
        <div>
          <div class="link-tile__type">
            <%= link.type %>
          </div>
          <div class="link-tile__title">
            <%= link.title %>
          </div>
        </div>
        <div class="link-tile__source">
          <%= link.source %>
        </div>
      </a>
    <% end %>
  </section>

  <footer class="component">
    <div class="footer-introduction">
      <h3 class="footer-introduction__title">
        <%= current_page.data.related.title %>
      </h3>
    </div>

    <% current_page.data.related.articles.each do |article| %>
      <a class="related-tile" href="<%= article.link %>">
        <img class="related-tile__image" src="<%= article.image %>" alt="<%= article.type %>">
        <div class="related-tile__text-content">
          <div>
            <div class="related-tile__type">
              <%= article.type %>
            </div>
            <div class="related-tile__title">
              <%= article.title %>
            </div>
          </div>
          <% if article.author %>
            <div class="related-tile__author">
              <%= article.author %>
            </div>
          <% end %>
        </div>
      </a>
    <% end %>

    <section class="footer-social-icons">
      <div>
        <a class="footer-social-icons__icon-container" href="http://twitter.com/Ubxd" target="_blank">
          <div class="footer-social-icons__icon">
            <%= image_tag 'twitter.png', class: 'footer-social-icons__image', alt: 'Twitter' %>
          </div>
        </a>
        <a class="footer-social-icons__icon-container" href="https://www.facebook.com/pages/Unboxed-Consulting/114802528557320" target="_blank">
          <div class="footer-social-icons__icon">
            <%= image_tag 'facebook.png', class: 'footer-social-icons__image', alt: 'Facebook' %>
          </div>
        </a>
        <a class="footer-social-icons__icon-container" href="http://www.linkedin.com/company/unboxed-consulting" target="_blank">
          <div class="footer-social-icons__icon">
            <%= image_tag 'linkedin.png', class: 'footer-social-icons__image', alt: 'LinkedIn' %>
          </div>
        </a>
      </div>
    </section>
  </footer>
</div>
